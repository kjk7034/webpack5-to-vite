{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/extensions": "off",
    "react/jsx-props-no-spreading": 0,
    "react/prop-types": 0,
    "import/no-dynamic-require": 0,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }]
  }
}
